
<?xml version="1.0"?>
<rdf:RDF xmlns="https://gitlab.laas.fr/discuter/overworld_tutorials/-/blob/master/ontologies/env#"
     xmlns:env="https://gitlab.laas.fr/discuter/overworld_tutorials/-/blob/master/ontologies/env#"
     xmlns:cg_objects="https://github.com/RIS-WITH/common_ground_ontology/blob/master/CG_objects#"
     xmlns:cg_entities="https://github.com/RIS-WITH/common_ground_ontology/blob/master/CG_entities#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="https://gitlab.laas.fr/discuter/overworld_tutorials/-/blob/master/ontologies/env">
      <owl:imports rdf:resource="https://github.com/RIS-WITH/common_ground_ontology/blob/master/CG_objects"/>
    </owl:Ontology>
    
    
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    
    
    <!-- env#Frame -->
    
    <owl:Class rdf:about="env#Frame">
        <rdfs:subClassOf rdf:resource="cg_objects#Object"/>
        <cg_entities:hasMesh rdf:datatype="http://www.w3.org/2001/XMLSchema#string">package://overworld_tutorials/models/frame.obj</cg_entities:hasMesh>
    </owl:Class>
    
    
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    
    
    <!-- env#frame_obj -->
    
    <owl:NamedIndividual rdf:about="env#frame_obj">
        <rdf:type rdf:resource="env#Frame"/>
    </owl:NamedIndividual>
    
    
    
    <!-- env#frame_stl -->
    
    <owl:NamedIndividual rdf:about="env#frame_stl">
        <rdf:type rdf:resource="env#Frame"/>
        <cg_entities:hasMesh rdf:datatype="http://www.w3.org/2001/XMLSchema#string">package://overworld_tutorials/models/frame.stl</cg_entities:hasMesh>
        <cg_entities:hasDiffuseTexture rdf:datatype="http://www.w3.org/2001/XMLSchema#string">package://overworld_tutorials/models/frame.png</cg_entities:hasDiffuseTexture>
    </owl:NamedIndividual>
    
    
    
    <!-- env#frame_green -->
    
    <owl:NamedIndividual rdf:about="env#frame_green">
        <rdf:type rdf:resource="env#Frame"/>
        <cg_entities:hasMesh rdf:datatype="http://www.w3.org/2001/XMLSchema#string">package://overworld_tutorials/models/frame_base.obj</cg_entities:hasMesh>
        <cg_entities:hasDiffuseTexture rdf:datatype="http://www.w3.org/2001/XMLSchema#string">package://overworld_tutorials/models/frame.png</cg_entities:hasDiffuseTexture>
        <cg_entities:hasColor rdf:resource="cg_entities#green"/>
    </owl:NamedIndividual>
    
    
    
    <!-- env#frame_red -->
    
    <owl:NamedIndividual rdf:about="env#frame_red">
        <rdf:type rdf:resource="env#Frame"/>
        <cg_entities:hasMesh rdf:datatype="http://www.w3.org/2001/XMLSchema#string">package://overworld_tutorials/models/frame.stl</cg_entities:hasMesh>
        <cg_entities:hasDiffuseTexture rdf:datatype="http://www.w3.org/2001/XMLSchema#string">package://overworld_tutorials/models/frame.png</cg_entities:hasDiffuseTexture>
        <cg_entities:hasColor rdf:resource="cg_entities#red"/>
    </owl:NamedIndividual>
    
</rdf:RDF>

