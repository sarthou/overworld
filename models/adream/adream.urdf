    <visual>
      <origin rpy="0 0 1.57" xyz="0 0 0.0"/>
      <geometry>
        <mesh filename="package://overworld/models/adream/floor.obj" scale="1 1 1"/>
      </geometry>
      <material name="floor_material"/>
    </visual>
    <collision>
      <contact_coefficients mu="1000.0" kp="0.00" kd="0.0"/>
      <origin rpy="0 0 1.57" xyz="0 0 0.0"/>
      <geometry>
        <mesh filename="package://overworld/models/adream/floor.obj" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

