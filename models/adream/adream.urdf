<?xml version="0.0" ?>
<robot name="adream">

  <material name="wall_material">
    <texture filename="package://overworld/models/textures/adream_wall_texture.png"/>
  </material>

  <material name="floor_material">
    <texture filename="package://overworld/models/textures/adream_floor_texture.png"/>
  </material>

  <link concave="yes" name="adream_walls">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value=".0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 1.57" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://overworld/models/adream/walls.obj" scale="1 1 1"/>
      </geometry>
      <material name="wall_material"/>
    </visual>
    <collision>
      <origin rpy="0 0 1.57" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://overworld/models/adream/walls_vhacd.obj" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <link name="adream_floor">
    <contact>
      <lateral_friction value="1"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value=".0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
